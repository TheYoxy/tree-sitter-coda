{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "coda",
      "camelcase": "Coda",
      "title": "Coda",
      "scope": "source.coda",
      "file-types": [
        "cod"
      ],
      "injection-regex": "^cod$",
      "class-name": "TreeSitterCoda"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "CODA (Coded Statement of Account)",
    "authors": [
      {
        "name": "Floryan Simar",
        "email": "floryan.simar@endevops.be",
        "url": "https://theyoxy.be/"
      }
    ],
    "links": {
      "repository": "https://github.com/theyoxy/tree-sitter-coda"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
